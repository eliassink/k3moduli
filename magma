K<t> := FunctionField(RationalField());
KP2<a,b,c> := ProjectiveSpace(K,2);
f := t^2*a^2*c - t*a^2*c - b^3 + t*b^2*c \
+ a*b^2 - t*a*b*c + t*a*c^2 - t^2*a*c^2;
C := Curve(KP2,f);
E,phi := EllipticCurve(C,C![1,1,1]);
s2 := phi(C![1,t,t]);
s3 := phi(C![1,1,0]);

E;
s2;
s3;
Order(s2);
Order(s3);
KodairaSymbols(E);
MWtors := TorsionSubgroup(E);
MWtors;
HeightPairing(s2,s2);
IsIsomorphic(EllipticCurve([t,-(t-1)^2*t,-(t-1)^2*t^2,0,0]),E);
